<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.immregistries.vfa.connect.model">
  <class name="Recommend" table="recommend">
    <id name="recommendId" column="recommend_id">
      <generator class="native"/>
    </id>
    <property name="recommendText" column="recommend_text" type="string"/>
    <property name="recommendTypeCode" column="recommend_type_code" type="string"/>
    <property name="recommendRangeCode" column="recommend_range_code" type="string"/>
  </class>
</hibernate-mapping>