<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.tch.fc.model">
  <class name="AssociatedDate" table="associated_date">
    <id name="associatedDateId" column="associated_date_id">
      <generator class="native"/>
    </id>
    <many-to-one name="testEvent" column="test_event_id" class="org.tch.fc.model.TestEvent"/>
    <property name="dateValue" column="date_value" type="date"/>
    <many-to-one name="dateRule" column="date_rule_id" class="org.tch.fc.model.RelativeRule"/>
    <property name="dateTypeCode" column="date_type_code" type="string"/>
  </class>
</hibernate-mapping>