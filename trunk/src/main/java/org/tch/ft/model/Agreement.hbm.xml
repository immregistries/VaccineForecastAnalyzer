<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.tch.ft.model">
  <class name="Agreement" table="agreement">
    <id name="agreementId" column="agreement_id">
      <generator class="native"/>
    </id>
    <property name="versionDate" column="version_date" type="date"/>
    <property name="agreementText" column="agreement_text" type="string"/>
  </class>
</hibernate-mapping>