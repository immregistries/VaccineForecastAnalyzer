<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.tch.fc.model">
  <class name="RelativeRule" table="relative_rule">
    <id name="ruleId" column="rule_id">
      <generator class="native"/>
    </id>
    <property name="timePeriodString" column="time_period" type="string"/>
    <property name="relativeToCode" column="relative_to_code" type="string"/>
    <many-to-one name="testEvent" column="test_event_id" class="org.tch.fc.model.TestEvent"/>
    <many-to-one name="andRule" column="and_rule_id" class="org.tch.fc.model.RelativeRule"/>
  </class>
</hibernate-mapping>