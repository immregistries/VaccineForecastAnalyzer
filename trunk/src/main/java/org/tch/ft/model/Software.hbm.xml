<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.tch.fc.model">
  <class name="Software" table="software">
    <id name="softwareId" column="software_id">
      <generator class="native"/>
    </id>
    <property name="label" column="label" type="string"/>
    <property name="serviceUrl" column="service_url" type="string"/>
    <property name="serviceType" column="service_type" type="string"/>
    <property name="scheduleName" column="schedule_name" type="string"/>
    <property name="visibleStatus" column="visible_status" type="string"/>
  </class>
</hibernate-mapping>