<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.tch.fc.model">
  <class name="RationaleGuidance" table="rationale_guidance">
    <id name="rationaleGuidanceId" column="rationale_guidance_id">
      <generator class="native"/>
    </id>
    <many-to-one name="rationale" column="rationale_id" class="org.tch.fc.model.Rationale"/>
    <many-to-one name="guidance" column="guidance_id" class="org.tch.fc.model.Guidance"/>
  </class>
</hibernate-mapping>