<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.tch.fc.model">
  <class name="Resource" table="resource">
    <id name="resourceId" column="resource_id">
      <generator class="native"/>
    </id>
    <property name="resourceText" column="resource_text" type="string"/>
    <property name="resourceLink" column="resource_link" type="string"/>
  </class>
</hibernate-mapping>