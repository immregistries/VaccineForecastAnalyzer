<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.tch.ft.model">
  <class name="SystemProperty" table="system_property">
    <id name="systemPropertyId" column="system_property_id">
      <generator class="native"/>
    </id>
    <property name="propertyName" column="property_name" type="string"/>
    <property name="propertyValue" column="property_value" type="string"/>
  </class>
</hibernate-mapping>