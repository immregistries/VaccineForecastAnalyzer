<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.tch.fc.model">
  <class name="Rationale" table="rationale">
    <id name="rationaleId" column="rationale_id">
      <generator class="native"/>
    </id>
    <property name="rationaleText" column="rationale_text" type="string"/>
  </class>
</hibernate-mapping>