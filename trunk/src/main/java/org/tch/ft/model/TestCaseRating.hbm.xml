<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.tch.ft.model">
  <class name="TestCaseRating" table="test_case_rating">
    <id name="testCaseRatingId" column="test_case_rating_id">
      <generator class="native"/>
    </id>
    <many-to-one name="testCase" column="test_case_id" class="org.tch.fc.model.TestCase"/>
    <many-to-one name="expertRating" column="expert_rating_id" class="ExpertRating"/>
  </class>
</hibernate-mapping>