<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.tch.fc.model">
  <class name="TestCaseSetting" table="test_case_setting">
    <id name="settingId" column="setting_id">
      <generator class="native"/>
    </id>
    <many-to-one name="testCase" column="test_case_id" class="org.tch.fc.model.TestCase"/>
    <many-to-one name="serviceOption" column="option_id" class="ServiceOption"/>
    <property name="optionValue" column="option_value" type="string"/>
  </class>
</hibernate-mapping>