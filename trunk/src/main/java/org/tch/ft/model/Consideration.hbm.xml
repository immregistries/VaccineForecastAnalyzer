<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.tch.fc.model">
  <class name="Consideration" table="consideration">
    <id name="considerationId" column="consideration_id">
      <generator class="native"/>
    </id>
    <property name="considerationText" column="consideration_text" type="string"/>
    <property name="considerationTypeCode" column="consideration_type_code" type="string"/>
  </class>
</hibernate-mapping>